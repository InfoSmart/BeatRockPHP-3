CREATE TABLE IF NOT EXISTS {DB_ALIAS}site_config 
(
  var varchar(100) NOT NULL,
  result text NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COMMENT='Configuración';

