CREATE TABLE IF NOT EXISTS {DB_ALIAS}site_backups_servers (
  id int(5) NOT NULL AUTO_INCREMENT,
  host varchar(50) NOT NULL,
  port int(5) NOT NULL,
  username varchar(50) NOT NULL,
  password varchar(50) NOT NULL,
  directory varchar(100) NOT NULL,
  last int(100) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 COMMENT='Servidores de recuperación.' AUTO_INCREMENT=1;

CREATE TABLE {DB_ALIAS}site_cache 
(
  id INT( 100 ) NOT NULL AUTO_INCREMENT ,
  page VARCHAR( 50 ) NOT NULL ,
  time INT( 100 ) NOT NULL DEFAULT  '12',
  PRIMARY KEY (id)
) ENGINE=MYISAM DEFAULT CHARSET=latin1 COMMENT='Caché';

CREATE TABLE IF NOT EXISTS {DB_ALIAS}site_countrys 
(
  id int(11) NOT NULL AUTO_INCREMENT,
  isoNum smallint(6) DEFAULT NULL,
  code2 char(2) DEFAULT NULL,
  code3 char(3) DEFAULT NULL,
  name varchar(80) DEFAULT NULL,
  PRIMARY KEY (id)
) ENGINE=MYISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=241;

